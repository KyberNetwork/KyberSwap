    "build_auto_dev": "NODE_ENV=dev node webpack-auto.js",
    "build_auto_ropsten": "NODE_ENV=ropsten node webpack-auto.js",
    "build_auto_production": "NODE_ENV=production node webpack-auto.js",
    "build_auto_staging": "NODE_ENV=staging node webpack-auto.js",
    "build_auto_staging_limit_order": "NODE_ENV=staging_limit_order node webpack-auto.js",
    "build_auto_all": "npm run build_auto_dev & npm run build_auto_ropsten & npm run build_auto_production & npm run build_auto_staging & npm run build_auto_staging_limit_order"
